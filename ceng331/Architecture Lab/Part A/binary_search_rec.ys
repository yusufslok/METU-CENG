binary_search:
    rrmovq %rdi, %rcx
    rrmovq %rsi, %r9
    subq %rdi, %r9
    irmovq $16, %r10
    dvvq %r10, %r9
    irmovq $8, %r10
    mllq %r10, %r9
    addq %r9, %rcx
    rrmovq %rsi, %r9
    subq %rdi, %r9
    jle rett
    rrmovq %rdx, %r9
    mrmovq (%rcx), %r10
    subq %r10, %r9
    je elseret1
    jg elseret2
    rrmovq %rcx, %rsi
    call binary_search
    ret
elseret1:
    subq %rdi, %rcx
    irmovq $8, %r9
    addq %r9, %rcx
    dvvq %r9, %rcx
    rrmovq %rcx, %rax
    ret
elseret2:
    pushq %rdi
    pushq %rsi
    pushq %rdx
    pushq %rcx
    irmovq $8, %r9
    addq %r9, %rcx
    rrmovq %rcx, %rdi
    call binary_search
    popq %rcx
    popq %rdx
    popq %rsi
    popq %rdi
    irmovq $-1, %r9
    rrmovq %rax, %r10
    subq %r9, %r10
    jne if1
    ret
if1:
    subq %rdi, %rcx
    irmovq $8, %r9
    dvvq %r9, %rcx
    addq %rax, %rcx
    irmovq $1, %r9
    addq %r9, %rcx
    rrmovq %rcx, %rax
    ret
rett:
    irmovq $-1, %rax
    ret





